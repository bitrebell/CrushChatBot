{
    "name": "Crush Chat Bot",
    "description": "A simple Chat-Bot written in Pyrogram using MongoDB",
    "logo": "https://telegra.ph/file/78bc88b883b22024002cd.jpg",
    "keywords": [
      "Crush",
      "mongodb",
      "python",
      "pyrogram"
    ],
      "buildpacks": [{
      "url": "heroku/python"
    }],
    "formation": {
      "worker": {
        "quantity": 1,
        "size": "free"
      }
    },
    "repository": "https://github.com/Darkranger00/CrushChatBot",
    "env": {
      "API_ID": {
        "description": "Get this value from my.telegram.org/apps.",
        "required": false,
        "value": ""
      },
      "API_HASH": {
        "description": "Get this value from my.telegram.org/apps.",
        "required": false,
        "value": ""
      },
      "BOT_TOKEN": {
        "description": "A Bot token from @BotFather",
        "required": true,
        "value": ""
      }
    }
  }
